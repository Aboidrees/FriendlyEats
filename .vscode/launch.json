  "version": "0.2.0",
  "configurations": [
    {
      "name": "Friendly Eats Web",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "flutterMode": "debug",
      "deviceId": "Edge",
      "args": [
        "--web-renderer",
        "html"
      ]
    },
    {
      "name": "Friendly Eats Mobile",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "flutterMode": "debug",
      "deviceId": "sdk gphone64 x86 64",
    },
    {
      "name": "friendlyeats (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "friendlyeats (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}